    "react": "17.0.1",
    "react-device-frame": "^2.1.4",
    "react-dom": "17.0.1",
    "react-intersection-observer": "^8.31.0",
    "react-particles-js": "^3.6.0",
    "react-scroll": "^1.8.1",
    "react3dtagcloud": "^0.2.2",
    "tailwindcss-filters": "^3.0.0",
    "tsparticles": "^1.37.4"
  },
  "devDependencies": {
    "rustywind": "0.6.7",
    "tailwindcss": "1.9.6"
